<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:divideStyle="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/content_bg">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <com.dearzs.app.widget.CustomCellSwitchView
            android:id="@+id/setting_item_push"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cRDescTextColor="@color/black_1"
            divideStyle:cRShowIcon="false"
            divideStyle:cRShowLine="true"
            divideStyle:cRTitle="消息推送"
            divideStyle:cRTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_privicy_agreenment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="true"
            divideStyle:cShowTopLine="false"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="隐私协议"
            divideStyle:cTitleTextColor="@color/black"
            android:visibility="gone"/>

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_secutiry_agreenment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc=""
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="true"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="许可协议"
            divideStyle:cTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc=""
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="false"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="邀请好友使用第二诊室"
            divideStyle:cTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_clear_cache"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc="1.8M"
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="false"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="清除缓存"
            divideStyle:cTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_pay_help"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc=""
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="true"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="支付帮助"
            divideStyle:cTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_feedback"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc=""
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="true"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="反馈"
            divideStyle:cTitleTextColor="@color/black" />

        <com.dearzs.app.widget.CustomCellView
            android:id="@+id/setting_item_check_update"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            divideStyle:cDesc="当前版本：v1.0.0"
            divideStyle:cShowTopLine="false"
            divideStyle:cDescTextColor="@color/black_1"
            divideStyle:cShowArror="false"
            divideStyle:cShowIcon="false"
            divideStyle:cShowLine="true"
            divideStyle:cTitle="检测更新"
            divideStyle:cTitleTextColor="@color/black" />

        <!--<Button-->
        <!--android:id="@+id/setting_exit_app"-->
        <!--style="@style/wfill_hwrap_style"-->
        <!--android:layout_marginLeft="20dp"-->
        <!--android:layout_marginRight="20dp"-->
        <!--android:layout_marginTop="30dp"-->
        <!--android:background="@drawable/rounded_gray_bg_white"-->
        <!--android:gravity="center"-->
        <!--android:padding="@dimen/dimens_10"-->
        <!--android:text="退出当前账号"-->
        <!--android:textColor="@drawable/common_blue_text_color_selector"-->
        <!--android:textSize="@dimen/dimens_16" />-->
    </LinearLayout>
</ScrollView>

